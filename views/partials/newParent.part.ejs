<div class="issue-root" style="margin-left: 2rem">
    <div class="issue-container">
        <i class="fold-button fa fa-chevron-down fa-fw"></i> 
        <span class="bracket">&lt;</span><%- include('./issueText.part.ejs', {issue: issue, project: project}) %><span class="bracket">&gt;</span>
        <div class="bar"><%- include('./progressBarPart.part.ejs', {issue: issue, step: 'todo'}) %><%- include('./progressBarPart.part.ejs', {issue: issue, step: 'doing'}) %><%- include('./progressBarPart.part.ejs', {issue: issue, step: 'done'}) %></div>
    </div>
    <div class="issue-root" style="margin-left: 2rem">
        <i class="fa fa-fw"></i> <span class="bracket">&lt;</span><div class="issue-text" data-issue-key="issue-form"><span class="issueType"><div class="input-holder"><span class="hidden-input"></span><input type="text" class="slick" placeholder="userstory"></div></span> 
            <select class="fa-option" onchange="optionUpdate(this)"><option selected value="todo" class="fa-option todo-option">&#xf0eb;</option><option value="doing" class="fa-option doing-option">&#xf017;</option><option value="done" class="fa-option done-option">&#xf00c;</option></select>
            <span class="attribute">id</span>=<span class="value">"..."</span> 
            <span class="attribute">title</span>=<span class="value">"<div class="input-holder"><span class="hidden-input"></span><input type="text" class="slick" placeholder="title"></div>"</span> 
            <span class="attribute">estimation</span>=<span class="value">"<div class="input-holder"><span class="hidden-input"></span><input type="text" class="slick" placeholder="5"></div>"</span> 
            <i class="fa fa-xl fa-check-square-o send-button"></i></div><span class="bracket">&#47;&gt;</span>
    </div>
    <i class="fa fa-fw"></i> <span class="bracket">&lt;&#47;</span><span class="issueType"><%= issue.issueType %></span><span class="bracket">&gt;</span>
</div>